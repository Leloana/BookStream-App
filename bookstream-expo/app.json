{
  "expo": {
    "name": "BookStream",
    "slug": "bookstream-expo",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bookstreamexpo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marcelo_ferreira.bookstream"
    },
    "android": {
      "package": "com.marcelo_ferreira.bookstream",
      "versionCode": 3,
      "adaptiveIcon": {
        "backgroundColor": "#5452FA",
        "foregroundImage": "./assets/images/icon-sem-fundo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "127be406-b1cd-4c46-b752-fd1f3b2aff1c"
      }
    }
  }
}
